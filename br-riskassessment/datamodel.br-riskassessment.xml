            <event_listeners>
                <event_listener id="CheckBCM" _delta="define">
                    <event>EVENT_DB_CHECK_TO_WRITE</event>
                    <callback>CheckBCMOnUpdate</callback>
                    <rank>1</rank>
                </event_listener>
            </event_listeners>
            <methods>
                <method id="CheckBCMOnUpdate">
                    <static>false</static>
                    <access>public</access>
                    <type>EventListener</type>
                    <code>
                        <![CDATA[public function CheckBCMOnUpdate(Combodo\iTop\Service\Events\EventData $oEventData)
{
    // Retrieve changes and check for presence of relevant fields in a single operation
    $aChanges = $this->ListChanges();
    $bRTOChanged = isset($aChanges['bcm_rto']);
    $bMTDChanged = isset($aChanges['bcm_mtd']);

    // If RTO or MTD changed, validate their relationship
    if ($bRTOChanged || $bMTDChanged) {
        $iRTO = intval($this->Get('bcm_rto'));
        $iMTD = intval($this->Get('bcm_mtd'));

        // Check if MTD is less than RTO when RTO is greater than zero
        if ($iRTO > 0 && $iMTD < $iRTO) {
            $this->AddCheckIssue(Dict::Format('Class:FunctionalCI/Error:MtdMustBeGreaterThanRto'));
        }
    }
}]]>
                    </code>
                </method>
            </methods>
